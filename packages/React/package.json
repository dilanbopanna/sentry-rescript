{
  "name": "@rescript-sentry/react",
  "version": "0.0.1",
  "private": false,
  "description": "This library provides Rescript Bindings for sentry-react",
  "scripts": {
    "re:clean": "rescript clean",
    "re:start": "rescript build -w",
    "publish:react": "cd packages/React && npm publish --access public --scope=@sentry-rescript",
    "publish:browser": "cd packages/Browser && npm publish --access public --scope=@sentry-rescript",
    "publish:types": "cd packages/Types && npm publish --access public --scope=@sentry-rescript"
  },
  "author": "Dilan Bopanna <bopannamj@gmail.com>",
  "contributors": [
    "Praful Koppalkar <praful.koppalkar@gmail.com>"
  ],
  "keywords": [
    "sentry",
    "rescript",
    "react"
  ],
  "files": [
    "/src",
    "/rescript.json"
  ],
  "devDependencies": {
    "rescript": "^10.1.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@rescript/react": "^0.11.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rescript": "^10.1.4",
    "@sentry/react": "^7.92.0"
  }
}
